APP_ENV=local
LOG_LEVEL=info

# Backend
API_HOST=0.0.0.0
API_PORT=8000
DATABASE_URL=postgresql+psycopg2://user:password@localhost:5432/zflow
POSTGRES_USER=zflow
POSTGRES_PASSWORD=zflow
POSTGRES_DB=zflow

# Worker
WORKER_POLL_INTERVAL_SECONDS=2

# External services (placeholders)
OBJECT_STORAGE_BUCKET=zflow-local

# Active providers
LLM_PROVIDER=openai
IMAGE_PROVIDER=stability
VIDEO_PROVIDER=vidu

# LLM providers
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GLM_API_KEY=your_glm_api_key_here
GLM_MODEL=glm-4.7

# Image providers
STABILITY_API_KEY=your_stability_api_key_here
DALLE_API_KEY=your_dalle_api_key_here
SEEDREAM_API_KEY=your_seedream_api_key_here
SEEDREAM_MODEL=doubao-seedream-4.5
SEEDREAM_API_BASE=https://open.bigmodel.cn/api/paas/v4
SEEDREAM_IMAGE_ENDPOINT=https://open.bigmodel.cn/api/paas/v4/images/generations
SEEDREAM_DEFAULT_SIZE=960x1280

# Video providers
VIDU_API_KEY=your_vidu_api_key_here
VIDU_API_BASE=https://open.bigmodel.cn/api/paas/v4
VIDU_VIDEO_ENDPOINT=https://open.bigmodel.cn/api/paas/v4/videos/generations
VIDU_ASYNC_RESULT_ENDPOINT=https://open.bigmodel.cn/api/paas/v4/async-result/{task_id}
VIDU_VIDEO_MODEL=viduq2-pro-img2video
VIDU_VIDEO_DEFAULT_SIZE=1280x720
RUNWAY_API_KEY=your_runway_api_key_here
PIKA_API_KEY=your_pika_api_key_here
SORA_API_KEY=your_sora_api_key_here
